{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/main.ts"
    }
  ],
  "build": {
    "env": {
      "DATABASE_URL": "@your-database-url"
    },
    "commands": [
      "npx prisma generate"
    ]
  }
}